{
  "name": "paciente-graphql",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "serve": "node dist/index.js",
    "watch-ts": "./node_modules/typescript/bin/tsc -w",
    "watch-node": "./node_modules/nodemon/bin/nodemon.js dist/index.js",
    "start": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare-data": "run-for-every-file --src './data/csv/' --file '*.csv' --run 'npx csvtojson ./data/csv/{{file-name-ext}} > ./data/json/{{file-name}}.json --ignoreEmpty=true --checkType=true --delimiter=;'"
  },
  "author": "yasser nascimento <yassern@protonmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/mongodb": "^3.1.28",
    "@types/node": "^12.0.2",
    "csvtojson": "^2.0.8",
    "husky": "^2.3.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "run-for-every-file": "^1.1.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "apollo-server": "^2.6.3",
    "graphql": "^14.3.1",
    "mongodb": "^3.2.7",
    "prisma-client-lib": "^1.34.3"
  }
}
